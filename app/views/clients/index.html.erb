<p id="notice"><%= notice %></p>

<h1>Listing Clients</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Surname</th>
      <th>Title</th>
      <th>Date of birth</th>
      <th>Employment status</th>
      <th>Marital status</th>
      <th>Maiden name</th>
      <th>Number of dependents</th>
      <th>Dependents names ages</th>
      <th>Nationality</th>
      <th>Planned retirement age</th>
      <th>Smoker</th>
      <th>Address 1</th>
      <th>Address 2</th>
      <th>Town</th>
      <th>Post code</th>
      <th>Country</th>
      <th>Preferred tel</th>
      <th>Home telephone</th>
      <th>Mobile telephone</th>
      <th>Best time</th>
      <th>Email</th>
      <th>Children under age</th>
      <th>Employment permanent</th>
      <th>Occupation</th>
      <th>Sick pay benefit</th>
      <th>User</th>
      <th>Controlling director</th>
      <th>Shareholding</th>
      <th>Year1 profit</th>
      <th>Year2 profit</th>
      <th>Year3 profit</th>
      <th>Accountant details</th>
      <th>Annual income</th>
      <th>Annual expenses</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.first_name %></td>
        <td><%= client.surname %></td>
        <td><%= client.title %></td>
        <td><%= client.date_of_birth %></td>
        <td><%= client.employment_status %></td>
        <td><%= client.marital_status %></td>
        <td><%= client.maiden_name %></td>
        <td><%= client.number_of_dependents %></td>
        <td><%= client.dependents_names_ages %></td>
        <td><%= client.nationality %></td>
        <td><%= client.planned_retirement_age %></td>
        <td><%= client.smoker %></td>
        <td><%= client.address_1 %></td>
        <td><%= client.address_2 %></td>
        <td><%= client.town %></td>
        <td><%= client.post_code %></td>
        <td><%= client.country %></td>
        <td><%= client.preferred_tel %></td>
        <td><%= client.home_telephone %></td>
        <td><%= client.mobile_telephone %></td>
        <td><%= client.best_time %></td>
        <td><%= client.email %></td>
        <td><%= client.children_under_age %></td>
        <td><%= client.employment_permanent %></td>
        <td><%= client.occupation %></td>
        <td><%= client.sick_pay_benefit %></td>
        <td><%= client.user %></td>
        <td><%= client.controlling_director %></td>
        <td><%= client.shareholding %></td>
        <td><%= client.year1_profit %></td>
        <td><%= client.year2_profit %></td>
        <td><%= client.year3_profit %></td>
        <td><%= client.accountant_details %></td>
        <td><%= client.annual_income %></td>
        <td><%= client.annual_expenses %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

